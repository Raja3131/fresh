{
  "expo": {
    "name": "fresh",
    "slug": "fresh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bg.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/bg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bg.png",
          "resizeMode": "cover"
        }
      ],
      [
        "expo-font",
        {
          "custom": {
            "Gilroy-Regular": "./assets/fonts/Gilroy-Regular.ttf",
            "Gilroy-Bold": "./assets/fonts/Gilroy-Bold.ttf",
            "Gilroy-Light": "./assets/fonts/Gilroy-Light.ttf",
            "Gilroy-SemiBold": "./assets/fonts/Gilroy-SemiBold.ttf",
            "Gilroy-Medium": "./assets/fonts/Gilroy-Medium.ttf"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
